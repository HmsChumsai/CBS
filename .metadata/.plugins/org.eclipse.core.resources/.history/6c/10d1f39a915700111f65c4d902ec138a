TEST(input)
	set ZTAB=$C(9)
	set ZENTER=$C(13)
	set ZPROBLEM=$C(10)
	
	type IO io1=Class.new("IO")
	set io1.fileName=$$TRNLNM^%ZFUNC("SCAU_SPOOL_EXTF")_"/"_"TESTZCOL.TXT"
	set io1.recordSize = 1024
	set io1.openParams="NEWV/WRITE"
	do io1.open()
	//set tWRITE = "BOO"_"|"_"COLTYP"_"|"_"COLSUBT"_"|"_"BOTCODE"_"|"_"SUBBOTCODE"_"|"_"SUBBOTDESC"_"|"_"SUMCOL"_"|"_"SUMCOLAMT"
	//do io1.write(tWRITE)
	set i=057
	while i<100 do {
				
		set ZDATA=^TMP("ZCONVCOL",input+i)
		if ZDATA[ZENTER set ZDATA=$TR(ZDATA,ZENTER," ")
		if ZDATA[ZTAB set ZDATA=$TR(ZDATA,ZTAB," ")
		if ZDATA[ZPROBLEM set ZDATA=$TR(ZDATA,ZPROBLEM," ")
		if ZDATA[$C(34) set ZDATA=$TR(ZDATA,$C(34)," ")
		do io1.write(ZDATA)
		set i=i+1	
		
		}
	do io1.close()
	quit

TESTRPT()

	set ZTAB=$C(9)
	set ZENTER=$C(13)
	set ZPROBLEM=$C(10)
	set ZRPT4="ZRPT4"
	type IO io4=Class.new("IO")
	set io4.fileName=$$TRNLNM^%ZFUNC("SCAU_SPOOL_EXTF")_"/"_"TESTRPT4_"_TJD_".TXT"
	set io4.recordSize = 1024
	set io4.openParams="NEWV/WRITE"
	do io4.open()
	set tWRITE = "BOO"_"|"_"COLTYP"_"|"_"COLSUBT"_"|"_"COLLID"_"|"_"COLLDETAIL"_"|"_"APPRAISEDBY"_"|"_"APPRAISEDDT"_"|"_"APPRAISALVAL"
	do io4.write(tWRITE)
	type ResultSet rs4 = Db.select("KEY1,KEY2,KEY3,KEY4,DATA","TMPRPT4","PID=:ZRPT4 AND KEY1=1 AND KEY2=1")
	set i=0
	while rs4.next() do { quit:i=100
		set i=i+1
		set tWRITE.piece("|",1) = rs4.getCol(1)
		set tWRITE.piece("|",2) = rs4.getCol(2)
		set tWRITE.piece("|",3) = rs4.getCol(3)
		set tWRITE.piece("|",4) = rs4.getCol(4)
		set tWRITE.piece("|",5) = rs4.getCol(5)	
		if tWRITE[ZENTER set ZDATA=$TR(tWRITE,ZENTER," ")
		if tWRITE[ZTAB set ZDATA=$TR(tWRITE,ZTAB," ")
		if tWRITE[ZPROBLEM set ZDATA=$TR(tWRITE,ZPROBLEM," ")
		if tWRITE[$C(34) set ZDATA=$TR(tWRITE,$C(34)," ")	
		do io4.write(tWRITE)
		}
	do io4.close()
	quit
	
ZCOUNT()
	set i=0
	set x=$O(^TMP("ZRPT1",""))
	while x do {
	//	if ^TMP("ZRPT1",x).piece(
