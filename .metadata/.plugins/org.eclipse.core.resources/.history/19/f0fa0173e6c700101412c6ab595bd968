	if 'Db.isDefined("DTJNA","TJD=:TJD,CLS,GRP,TYP,CID=:zdtjfm.cid") do {
		set ztmp.pid="ZQ"
		set ztmp.key1="NON"
		set ztmp.key1=zdtjfm.tjd
		set ztmp.key2=zdtjfm.cls
		set ztmp.key3=zdtjfm.cid
		set ztmp.DATA=zdtjfm.cid
		do ztmp.bypassSave()
	}
	
	
