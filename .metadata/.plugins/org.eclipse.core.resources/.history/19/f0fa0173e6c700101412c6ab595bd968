	//set i=1
	type RecordTMPRPT4 ztmp=Class.new("RecordTMPRPT4")
	type RecordDTJFM zdtjfm=Db.getRecord("DTJFM","TJD=:TJD,CLS,GRP,TYP,CID,TSEQ")
