	set Z="ZQ"
	type public String ZFILE
	type IO io = Class.new("IO")	
	set io.fileName="ZQII2.txt"
	set io.directory=$$TRNLNM^%ZFUNC("SCAU_SPOOL")
	set io.openParams="NEWV/WRITE"
	set io.recordSize=1024
	do io.open()
	
	set Z1="NEW"
	type ResultSet tmp=Db.select("DATA,PID","TMPRPT3","PID=:Z AND KEY1=:Z1")
	while tmp.next() do {
		set ZFILE=tmp.getCol("DATA")
		set ZW="New Account: "_ZFILE
		do io.write(ZW)
	}
	set Z2="NON"
	type ResultSet tmp=Db.select("DATA,PID","TMPRPT3","PID=:Z AND KEY1=:Z2 ")
	while tmp.next() do {
		set ZFILE=tmp.getCol("DATA")
		set ZW="Non-Financial: "_ZFILE
		do io.write(ZW)
	}
	set Z3="NEW"
	type ResultSet tmp=Db.select("DATA,PID","TMPRPT3","PID=:Z AND KEY1=:Z3")
	while tmp.next() do {
		set ZFILE=tmp.getCol("DATA")
		set ZW="New Account & Non-Financial: "_ZFILE
		do io.write(ZW)
	}	

	set IOTYP="RMS"	
	do io.close()
	
	
