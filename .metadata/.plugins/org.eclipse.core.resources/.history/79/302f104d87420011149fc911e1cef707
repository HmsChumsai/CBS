	type RecordZCONTRACT zcnt = Db.getRecord("ZCONTRACT","CTRCT=:CTRCT")
	type RecordTMPRPT2 ext = Class.new("RecordTMPRPT2")
	type RecordTMPRPT2 ext2 = Class.new("RecordTMPRPT2")
	
	//Extract ALL
		set ext.PID = "ZCONVCNT"
		set ext.KEY1 = zcnt.ctrct
		set ext.KEY2 = 0
	
			set ZDATA.piece("|",1)= zcnt.BOO
			set ZDATA.piece("|",2) = zcnt.CC
			set ZDATA.piece("|",3) = zcnt.CONTRREL
			set ZDATA.piece("|",4) = zcnt.CONVDT.toString("YEAR-MM-DD")
			set ZDATA.piece("|",5) = zcnt.CRTDATE.toString("YEAR-MM-DD")
			set ZDATA.piece("|",6) = zcnt.CRTUSER
			set ZDATA.piece("|",7) = zcnt.CTRCT
			set ZDATA.piece("|",8) = zcnt.CTRCTDT.toString("YEAR-MM-DD")_" 0:00:00.0"
			set ZDATA.piece("|",9) = zcnt.CTRSTAT
			set ZDATA.piece("|",10) = zcnt.MCTRCT
			set ZDATA.piece("|",11) = zcnt.PLDGAMT
			set ZDATA.piece("|",12) = zcnt.PLDGSEQ
			set ZDATA.piece("|",13) = zcnt.RELDATE.toString("YEAR-MM-DD")_" 0:00:00.0"
			set ZDATA.piece("|",14) = zcnt.REMARKS
			set ZDATA.piece("|",15) = zcnt.RESPUNIT
			
		set ext.DATA=ZDATA
		do ext.save()
	
	if zcnt.CTRSTAT=0 do {
		set ext2.PID = "ZCONVCNT"
		set ext2.KEY1 = zcnt.ctrct
		set ext2.KEY2 = 1
		set ext2.DATA=ZDATA
		do ext2.save()
