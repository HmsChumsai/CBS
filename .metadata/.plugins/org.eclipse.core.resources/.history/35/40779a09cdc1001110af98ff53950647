MKTAUTH
	
	type String tWRITE
	////set ZEXTDAT = $$DAT^%ZM(TJD,"YEARMMDD")
	type IO io1=Class.new("IO")
	set io1.fileName=$$TRNLNM^%ZFUNC("SCAU_SPOOL_EXTF")_"/"_"zmk/ZUTBLMKTAUTH_"_ZEXTDAT_".txt"
	set io1.openParams="NEWV/WRITE"
	do io1.open()
	set tWRITE = "MARKET_CODE_CODE|BRANCH_CODE"
	do io1.write(tWRITE)
	set i=0
	type DbSet ds = Db.selectDbSet("ZUTBLMKTAUTH")
	while ds.next() do {
		type String ZCHECK,output
		set i=i+1
		set ZCHECK=ds.getRecord().BRCD
		if 'Db.isDefined("UTBLBRCD","BRCD=:ZCHECK") quit
		
		set output.piece("|",1) = ds.getRecord().ZMKTCD
		set output.piece("|",2) = ds.getRecord().BRCD
		do io1.write(output)
		}
	do io1.write("Total "_i_"|")
	do io1.close()
			
	quit
	
		

	
	
	
	
