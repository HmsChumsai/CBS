	type String tWRITE
	type IO io1=Class.new("IO")
	set io1.fileName=$$TRNLNM^%ZFUNC("SCAU_SPOOL_EXTF")_"/"_"zmk/UTBLNBD1_"_ZEXTDAT_".txt"
	set io1.openParams="NEWV/WRITE"
	do io1.open()
	set tWRITE = "CALENDAR_NBD_CODE|NON_BUSINIESS_DT"
	do io1.write(tWRITE)
	set i=0
	type DbSet ds = Db.selectDbSet("UTBLNBD1")
	while ds.next() do {
		set i=i+1
		type String output
		set output.piece("|",1) = ds.getRecord().NBDC
		set output.piece("|",2) = ds.getRecord().NBD.toString("DD/MM/YEAR")	
		
		do io1.write(output)
		
	}
	
	do io1.write("Total "_i_"|")
	do io1.close()
			
	quit
	
RFLG

	type String tWRITE
	type IO io1=Class.new("IO")
	set io1.fileName=$$TRNLNM^%ZFUNC("SCAU_SPOOL_EXTF")_"/"_"zmk/MT_CONSTRAINT_"_ZEXTDAT_".txt"
	set io1.openParams="NEWV/WRITE"
	do io1.open()
	set tWRITE = "CODE|DESC_TH|DESC_EN"
	do io1.write(tWRITE)
	set i=0
	type DbSet ds = Db.selectDbSet("UTBLRFLG","GRP='CIF'")
	while ds.next() do {
		set i=i+1
		type String output
		set output.piece("|",1) = ds.getRecord().RFLG
		set output.piece("|",2) = ds.getRecord().DESC	
		set output.piece("|",3) = ds.getRecord().FMDESC
		
		do io1.write(output)
		
	}
	
	do io1.write("Total "_i_"|")
	do io1.close()
			
	quit
	
CAMPR

	type String tWRITE
	type IO io1=Class.new("IO")
	set io1.fileName=$$TRNLNM^%ZFUNC("SCAU_SPOOL_EXTF")_"/"_"zmk/MR_CAMPAIGN_REL_"_ZEXTDAT_".txt"
	set io1.openParams="NEWV/WRITE"
	do io1.open()
	set tWRITE = "PRODUCT_SUB_TYPE_CODE|MARKET_CODE_CODE|CAMPAIGN_CODE"
	do io1.write(tWRITE)
	set i=0
	type DbSet ds = Db.selectDbSet("ZUTBLCAMREL")
	while ds.next() do {
		set i=i+1
		type String output

		set output.piece("|",1) = ds.getRecord().SUBTCD
		set output.piece("|",2) = ds.getRecord().MKTCD
		set output.piece("|",3) = ds.getRecord().CODE
		
		do io1.write(output)
		
	}
	
	do io1.write("Total "_i_"|")
	do io1.close()
			
	quit

OUBRCD

	type String tWRITE
	type IO io1=Class.new("IO")
	set io1.fileName=$$TRNLNM^%ZFUNC("SCAU_SPOOL_EXTF")_"/"_"zmk/ZUTBLOUBRCD_"_ZEXTDAT_".txt"
	set io1.openParams="NEWV/WRITE"
	do io1.open()
	set tWRITE = "OU|BRCD"
	do io1.write(tWRITE)
	set i=0
	type DbSet ds = Db.selectDbSet("ZUTBLOUBRCD")
	while ds.next() do {
		set i=i+1
		type String output
		set output.piece("|",1) = ds.getRecord().OU
		set output.piece("|",2) = ds.getRecord().BRCD
		
		do io1.write(output)
		
	}
	
	do io1.write("Total "_i_"|")
	do io1.close()
			
	quit

	
	
	

	
	
	
	

	
	




	
		
	
	
	
	

	
	





	