		set output.piece("|",2) = ds.getRecord().DESC
		set output.piece("|",3) = ds.getRecord().FMDESC_"|"
		do io1.write(output)
		}
	do io1.write("Total "_i_"|")
	do io1.close()
			
	quit
	
