	quit
	
EXTRACTALL
	
	do SYSVAR^SCADRV0()
	do AGENT
	do ARRPUR
	do BRCD
	//do BUDGETSETUP
	do CAMPR
	do CAMPSET
	do CAMPSETPR
	do CC
	do CCNTR
	do CNTRY
	do CNTRY1
	do COMLINE
	do CRCD
	do DIST
	do DISTCD
	do DLACD
	do DOB
	do EDUC
	do EFTINST
	do HRC
	do IC
	do ID
	do INDEX
	do INDEX1
	do ISICGC
	do ISICSD
	do ISICTS
	do LNAPASHLD
	do LNAPESTYP
	do LNAPRSTAT
	do LNAPRTYP
	do LNBD
	do LNBD1
	do LOC
	do MF
	do MINISTRY1
	do MINISTRY2
	do MINISTRY3
	do MKTAUTH
	do MKTCD
	do MKTCDSETUP
	do MOU
	do MS
	do MTCAMP
	do NATION
	do PCM
	do PERCONS
	do RELIGION
	do RFLG
	do PRODCTL
	do PRODLNK
	do PRODLNKCFG
	do PRODRLDT
	do PRODRTDT
	do RELIGION
	do RPAYMETH
	do SDISTCD
	do SMECD
	do SOC
	do SPLEND 
	do STATD
	do STBLBUSOPT
	do STBLCLS
	do STBLCNTRY
	do STBLCNTRY1
	do STBLGRP
	do STBLGRPSET
	do SUBT
	do SUBTSET
	do TITLE
	do UHHOLD
	do ZUTBLLNAPESOWN
	do OUBRCD

	
	quit



CCNTR

	type String tWRITE
	//////set ZEXTDAT = $$DAT^%ZM(TJD,"YEARMMDD")
	type IO io1=Class.new("IO")
	set io1.fileName=$$TRNLNM^%ZFUNC("SCAU_SPOOL_EXTF")_"/"_"zmk/UTBLCCNTR_"_ZEXTDAT_".txt"
	set io1.openParams="NEWV/WRITE"
	do io1.open()
	set tWRITE = "COST_CENTER_ID"_"|"_"DESCRIPTION"_"|"
	do io1.write(tWRITE)
	set i=0
	type DbSet ds = Db.selectDbSet("UTBLCCNTR")
	while ds.next() do {
		type String output
		set i=i+1
		set output=""
		set output.piece("|",1) = ds.getRecord().CC
		set output.piece("|",2) = ds.getRecord().DESC
		set output.piece("|",3) = ds.getRecord().FMDESC
		do io1.write(output)
		}
	do io1.write("Total "_i_"|")
	do io1.close()	
	quit
	
CNTRY	
	type String tWRITE
	////set ZEXTDAT = $$DAT^%ZM(TJD,"YEARMMDD")
	type IO io1=Class.new("IO")
	set io1.fileName=$$TRNLNM^%ZFUNC("SCAU_SPOOL_EXTF")_"/"_"zmk/STBLCNTRY_"_ZEXTDAT_".txt"
	set io1.openParams="NEWV/WRITE"
	do io1.open()
	set tWRITE = "COUNT_CODE"_"|"_"DESCRIPTION"_"|"
	do io1.write(tWRITE)
	set i=0
	type DbSet ds = Db.selectDbSet("STBLCNTRY")
	while ds.next() do {
		
		type String output
		set i=i+1
		set output.piece("|",1) = ds.getRecord().CNTRY
		set output.piece("|",2) = ds.getRecord().DESC_"|"
		do io1.write(output)
		}
	do io1.write("Total "_i_"|")
	do io1.close()			
	quit
	

DISTCD
	type String tWRITE
	//////set ZEXTDAT = $$DAT^%ZM(TJD,"YEARMMDD")
	type IO io1=Class.new("IO")
	set io1.fileName=$$TRNLNM^%ZFUNC("SCAU_SPOOL_EXTF")_"/"_"zmk/ZUTBLSDISTCD_"_ZEXTDAT_".txt"
	set io1.openParams="NEWV/WRITE"
	do io1.open()
	set tWRITE = "COUNTRY_CODE|PROVINCE_CODE|DISTRICT_CODE|SUB_DISTRICT_CODE|DESC_TH|DESC_EN"
	do io1.write(tWRITE)
	set i=0
	type DbSet ds = Db.selectDbSet("ZUTBLSDISTCD")
	while ds.next() do {
		
		type String output
		set i=i+1		
		set output.piece("|",1) = ds.getRecord().CNTRY
		set output.piece("|",2) = ds.getRecord().STATE
		set output.piece("|",3) = ds.getRecord().DIST
		set output.piece("|",4) = ds.getRecord().SUBDIST
		set output.piece("|",5) = ds.getRecord().DESC
		set output.piece("|",6) = ""
		do io1.write(output)
		}
	do io1.write("Total "_i_"|")
	do io1.close()	
		
	quit

	
CNTRY1	
	type String tWRITE
	////set ZEXTDAT = $$DAT^%ZM(TJD,"YEARMMDD")
